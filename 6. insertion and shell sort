def insertionSort(A):
    n=len(A)
    if n <= 1:
        return 0

    for i in range(1,n):
        key = A[i]
        j=i-1
        while j>=0 and key<A[j]:
            A[j+1] = A[j]
            j =j-1
        print(f"pass{i} : ",A)
        A[j+1] = key


A = [56,45,68,47,78,99,74,22,11,33]
print("Initial array : ",A)
insertionSort(A)
print("Sorted array : ",A)




#shell sort
A = [56, 45, 68, 47, 78, 99, 74, 22, 11, 33,5]
n = len(A)
mid = n // 2

print("The original array:", A)

while mid != 0:
    for i in range(mid, n):
        p = A[i]
        while i >= mid and A[i - mid] > p:
            A[i] = A[i - mid] 
            i = i-mid 
            A[i]=p 
    mid =mid// 2

print("The sorted array:", A)
